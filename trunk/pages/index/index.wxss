/* 打开app */
.open_app{ 
  width: 32.5%;
  height: 11.7vw;
  line-height: 11.7vw;
  text-align: center;
  background: #060606;
  color: #FFD98B;
  position: fixed;
  right: 0;
  bottom: 39.2vw;
  font-size: 4.3vw;
  border-top-left-radius: 100px;
  border-bottom-left-radius: 100px;
}

.art_content_img .video-play-btn{
  width: 16vw;
  height: 16vw;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -8vw;
  margin-top: -8vw;
}
/* 搜索框 */
.search_top_fixed{
  /* position: fixed;
  z-index: 999;
  top: 11.7vw; */
  width: 100%;
  height: auto;
  background: #ffffff;
  margin-top: 17vw
}
.index_search_box{
  width: 92%;
  height: 9.6vw;
  line-height: 9.6vw;
  margin: 5.3vw auto;
  background: #ffffff;
  box-shadow:0px 0px 2.13vw 0px rgba(0,0,0,0.06);
  border-radius: 4.8vw;
  position: relative;
}
.index_search_box image{
  width: 3.2vw;
  height: 3.2vw;
  position: absolute;
  left: 6.7vw;
  top: 3.2vw;
}
.index_search_box input{
  width: 87%;
  height: 100%;
  border-radius: 4.8vw;
  padding-left: 13%;
  font-size: 3.2vw;
}
/* 车主访谈 */
.car_inter_box{
  width: 100%;
  height: 36vw;
  overflow: hidden;
  margin: 0 auto;
  margin-bottom: 8vw;
