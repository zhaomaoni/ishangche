  width: 28.27vw;
}
.car_inter_left .car_name{
  text-align: center;
  color: #333;
  font-size: 4.27vw;
  font-weight: 500;
  padding-top: 6.4vw;
  padding-bottom: 1vw
}
.car_msg_box{
  width: 100%;
  overflow: hidden;
}
.car_msg_box .car_icon{
  float: left;
  margin-right: 2.27vw;
  height:3.73vw;
  overflow:hidden;
  line-height:3.73vw;
  /* width: 13vw; */
}
.car_msg_box .car_icon:last-child{
  margin-right: 0
}
.car_icon text{
  color: #999999;
  font-size: 3.2vw;
}
.car_main{
  font-size: 2.93vw;
  color: #999999;
  text-align: center;
  padding-bottom: 6.4vw
}
.car_icon image{
  width: 3.73vw;
  height: 3.73vw;
  margin-right: 2px;
  vertical-align: middle;
}
.car_inter_right{
  width: 52.4vw;
  height: 36vw;
  margin-left: 5.3vw;
  border-radius:4.27vw;
  overflow:hidden;
}
.car_inter_right image{
  width: 100%;
  height: 100%;
}
.art_left_img video{
  width: 100%
}
.art_left_img .video-cover{
  width: 100%;
  height: 100%;
  border-radius:4.27vw;
}
.art_left_img .video-play-btn{
  width:16vw;
